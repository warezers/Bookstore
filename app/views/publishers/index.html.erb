<h1>Publishers</h1>

<table class="table table-striped">
  <tr>
    <th>publisher Name</th>
    <th> Actions</th>
  </tr>
  <% @publishers.each do |publisher|%>
    <tr>
      <td><%= publisher.name%></td>
      <td><%= link_to 'Edit',edit_publisher_path(publisher.id)%> | <%= link_to 'Delete',publisher_path(publisher), method: :delete,data: {confirm: 'Are you sure delete?'}%></td>
    </tr>
  <% end %>
</table>
